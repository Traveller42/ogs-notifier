{
  "name": "Nova Notifier",
  "description": "Notifications from nova.gs Go server",
  "version": "1",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "http://online-go.com/*"
  ],
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon_60.png"
  }
}
