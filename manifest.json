{
  "name": "OGS Notifier",
  "description": "Notifications from Online-Go Server (online-go.com)",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": [
    "https://online-go.com/*"
  ],
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "icon_60.png"
  }
}
